        "persons = [       #this is a list of dictionaries\n",
        "    {\n",
        "        'name': 'Vuduc',    #name key\n",
        "        'age': 40,    #age key          #dictionary 1\n",
        "        'title': 'Data Scientist'   #title key\n",
        "    },\n",
        "    {\n",
        "        'name': 'Sokol',\n",
        "        'age': 45,      #dictionary 2\n",
        "        'title': 'Data Engineer'\n",
        "    },\n",
        "    {\n",
        "        'name': 'Wooley',\n",
        "        'age': 43,        #dictionary 3\n",
        "        'title': 'Program Director'\n",
        "    }\n",
        "]"
      ]
    },
