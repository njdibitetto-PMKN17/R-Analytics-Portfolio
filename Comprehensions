            "text/plain": [
              "[[1, 0, 0], [0, 1, 0], [0, 0, 1]]"
            ]
          },
          "metadata": {},
          "execution_count": 36
        }
      ],
      "source": [
        "cols = []\n",
        "rows = []\n",
        "for row in range(3):\n",
        "    for col in range(3):\n",
        "        if(col == row): #Row 1 Column 1 will append and make a 1, Row 1 column 2, will append a 0, row 1 column 3 append a 0 because not the same.\n",
        "            cols.append(1)\n",
        "        else:\n",
        "            cols.append(0)\n",
        "    rows.append(cols)\n",
        "    cols = []\n",
        "rows\n",
        "\n",
        "#Row 2 Column 1 will append and make a 0, Row 2 column 2, will append a 1, row 2 column 3 append a 0 because not the same.\n",
        "\n",
        "#Row 3 Column 1 will append and make a 0, Row 3 column 2, will append a 0, row 3 column 3 append a 1 .\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 37,
      "id": "c34440e5",
      "metadata": {
        "id": "c34440e5"
      },
      "outputs": [],
      "source": [
        "identity_matrix = [ [ 1 if item_idx == row_idx else 0 for item_idx in range(0, 3) ] for row_idx in range(0, 3) ]\n",
        "# format is [Output if-else, loop(inner for loop) outer for loop ]\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "[[1 if col == row else 0 for col in range(3)] for row in range(3)]\n",
        "# format is [Output if-else, loop(inner for loop) outer for loop ]"
      ],
      "metadata": {
